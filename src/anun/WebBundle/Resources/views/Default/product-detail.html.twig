{% extends '@anunWeb/Layout/base.html.twig' %}
{% block style %}
{% endblock %}
{% block body %}
    <div id="about-us">
        <div class="about-us-2 about-us-body  text-center">
            <div class="container">
                <div class="row">
                    <div class="col-xs-4  wow bounceIn">
                        <div class="swiper-container parallax-slider parallax-window" data-parallax="scroll"
                             data-speed="-0.1">
                            <div class="swiper-wrapper">
                                <div class="swiper-slide"
                                     style="background-image: url('{{ asset('resources/web/images/cover4.jpg') }}');background-position: center center;background-repeat: no-repeat;">
                                </div>
                            </div>
                            <!-- Add Arrows -->
                            <div class="swiper-button-next"><i class="fa fa-angle-right" aria-hidden="true"></i></div>
                            <div class="swiper-button-prev"><i class="fa fa-angle-left" aria-hidden="true"></i></div>
                        </div>


                    </div>

                    <div class="col-xs-8  wow bounceIn">

                    </div>
                </div>
            </div>
        </div>
    </div>
{% endblock %}
{% block js %}
{% endblock %}